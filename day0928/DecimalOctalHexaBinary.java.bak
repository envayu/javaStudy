class DecimalOctalHexaBinary 
{
	public static void main(String[] args) 
	{
		int a = 12; //아무표시없으면 10진수
		int b = 014; //0을 붙이면 8진수로 인식
		int c = 0xC; //0x를 붙이면 16진수로 인식

		System.out.println(a);
		System.out.println(b);
		System.out.println(c);


	}
}



